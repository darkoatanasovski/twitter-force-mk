{
  "manifest_version": 3,
  "name": "Twitter Force MK",
  "version": "1.0",
  "description": "Forces MK lang instead of BG for twitter web =)",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*.twitter.com/*",
        "https://twitter.com/*"
      ]
    }
  ]
}
